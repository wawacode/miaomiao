<?xml version='1.0' encoding='utf-8'?>
<widget id="com.miaomiao.bconsole" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="PushPlugin">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <feature name="Toast">
        <param name="ios-package" value="Toast" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="ios-package" value="CDVSound" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="CDVBarcodeScanner" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <preference name="CameraUsesGeolocation" value="false" />
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="CallNumber">
        <param name="ios-package" value="CallNumber" />
    </feature>
    <feature name="DatePicker">
        <param name="ios-package" value="DatePicker" />
    </feature>
    <icon height="180" src="resource/ios/icons/icon-60@3x.png" width="180" />
    <icon height="60" src="resource/ios/icons/icon-60.png" width="60" />
    <icon height="120" src="resource/ios/icons/icon-60@2x.png" width="120" />
    <icon height="76" src="resource/ios/icons/icon-76.png" width="76" />
    <icon height="152" src="resource/ios/icons/icon-76@2x.png" width="152" />
    <icon height="40" src="resource/ios/icons/icon-40.png" width="40" />
    <icon height="80" src="resource/ios/icons/icon-40@2x.png" width="80" />
    <icon height="57" src="resource/ios/icons/icon.png" width="57" />
    <icon height="114" src="resource/ios/icons/icon@2x.png" width="114" />
    <icon height="72" src="resource/ios/icons/icon-72.png" width="72" />
    <icon height="144" src="resource/ios/icons/icon-72@2x.png" width="144" />
    <icon height="29" src="resource/ios/icons/icon-small.png" width="29" />
    <icon height="58" src="resource/ios/icons/icon-small@2x.png" width="58" />
    <icon height="50" src="resource/ios/icons/icon-50.png" width="50" />
    <icon height="100" src="resource/ios/icons/icon-50@2x.png" width="100" />
    <splash height="480" src="resource/ios/splash/Default~iphone.png" width="320" />
    <splash height="960" src="resource/ios/splash/Default@2x~iphone.png" width="640" />
    <splash height="1024" src="resource/ios/splash/Default-Portrait~ipad.png" width="768" />
    <splash height="2048" src="resource/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
    <splash height="768" src="resource/ios/splash/Default-Landscape~ipad.png" width="1024" />
    <splash height="1536" src="resource/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
    <splash height="1136" src="resource/ios/splash/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" src="resource/ios/splash/Default-667h.png" width="750" />
    <splash height="2208" src="resource/ios/splash/Default-736h.png" width="1242" />
    <splash height="1242" src="resource/ios/splash/Default-Landscape-736h.png" width="2208" />
    <name>喵喵商家</name>
    <description>
        喵喵商家客户端
    </description>
    <author email="dev@lizi-inc.com" href="http://www.mbianli.com/">
        Miaomiao team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarBackgroundColor" value="white" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="BackupWebStorage" value="none" />
</widget>
